{
  "name": "fixool",
  "version": "0.1.0",
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.15.2",
    "@frctl/fractal": "^1.5.4",
    "@frctl/twig": "^1.2.6",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.3"
  },
  "scripts": {
    "fractal": "fractal start --sync",
    "build:css": "postcss src/css/styles.css -o public/css/styles.css",
    "watch": "watch 'npm run build:css' ./src/css",
    "start": "concurrently 'npm:watch' 'npm:fractal' "
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "postcss-easy-import": "^3.0.0"
  }
}
